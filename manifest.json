{
  "manifest_version": 3,
  "name": "Gmail Mail Distribution Dashboard",
  "version": "1.0",
  "description": "Analyzes your Gmail inbox and displays a dashboard of email distributions by sender.",
  "permissions": ["identity", "storage"],
  
  "icons": {
    "16": "icons/extension-icon-16.png",
    "48": "icons/extension-icon-48.png",
    "128": "icons/extension-icon-128.png"
  },
  "oauth2": {
    "client_id": "692565867759-09c8aoaonk2v5k00tchnl3qj02asfqdu.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/gmail.readonly", "https://www.googleapis.com/auth/calendar.readonly"]
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/extension-icon-16.png",
      "48": "icons/extension-icon-48.png",
      "128": "icons/extension-icon-128.png"
      }
  }
}
